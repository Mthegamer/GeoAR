<?xml version='1.0' encoding='utf-8'?>
<widget id="im.dnn.geoar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GeoAR</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <feature name="http://api.phonegap.com/1.0/device" />

    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="DisallowOverscroll" value="true" />

    <access origin="http://127.0.0.1*" />
    <access origin="*" />

    <gap:plugin name="org.apache.cordova.geolocation" version="0.3.6" />
    <gap:plugin name="com.wikitude.phonegap.WikitudePlugin.WikitudePlugin" />
</widget>
